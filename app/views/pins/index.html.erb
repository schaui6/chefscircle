<div class="transitions-enabled" id="pins">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
      <%= link_to (image_tag pin.image.url), pin %>
      <h2>
        <%= link_to pin.rececipe_name_with_serving_size, pin %>
      </h2>
      <p class="user">
        Submitted by
        <%= pin.user.username %>
      </p>
    </div>
  <% end %>
</div>
